<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".FragmentDashboardManager">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <Space
            android:layout_width="match_parent"
            android:layout_height="41dp"
            android:id="@+id/dbm_space_1" />

        <TextView
            android:id="@+id/dbm_welcome"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Welcome Insert Name Here" />

        <Space
            android:layout_width="match_parent"
            android:layout_height="45dp"
            android:id="@+id/dbm_space_2" />

        <TextView
            android:id="@+id/textView20"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Current Properties" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <Button
                android:id="@+id/dbm_back_c"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="Previous" />

            <TextView
                android:id="@+id/dbm_label_c"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="1/10" />

            <Button
                android:id="@+id/dbm_next_c"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="Next" />

        </LinearLayout>

        <TextView
            android:id="@+id/dbmg_addr_c"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="House at 2315 St" />

        <Button
            android:id="@+id/dbm_details_c"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Details" />

        <Space
            android:layout_width="match_parent"
            android:layout_height="38dp"
            android:id="@+id/dbm_space_3" />

        <TextView
            android:id="@+id/textView22"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Previously Owned Properties" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <Button
                android:id="@+id/dbm_back_p"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="Previous" />

            <TextView
                android:id="@+id/dbm_label_p"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="1/10" />

            <Button
                android:id="@+id/dbm_next_p"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="Next" />
        </LinearLayout>

        <TextView
            android:id="@+id/dbm_addr_p"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="House at 325 Main St" />

        <Button
            android:id="@+id/dbm_details_p"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Details" />

        <Space
            android:layout_width="match_parent"
            android:layout_height="55dp"
            android:id="@+id/dbm_space_4" />

        <TextView
            android:id="@+id/textView27"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Property Requests" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <Button
                android:id="@+id/dbm_accept_r"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="Accept" />

            <Button
                android:id="@+id/dbm_back_r"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="Back" />

            <TextView
                android:id="@+id/dbm_layout_r"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="1/3" />

            <Button
                android:id="@+id/dbm_next_r"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="Next" />

            <Button
                android:id="@+id/dbm_reject_r"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="Reject" />
        </LinearLayout>

        <TextView
            android:id="@+id/dbm_addr_r"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="HOUSE AT 25 BROOKE ST" />

        <TextView
            android:id="@+id/dbm_share_r"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Profit Share: 5%" />

        <TextView
            android:id="@+id/dbm_landlord_r"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Tom Scott (Landlord)" />

        <TextView
            android:id="@+id/dbm_msg_r"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Hi, I'd like to assign you as my property manager for this property" />

        <Button
            android:id="@+id/dbm_details_r"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Details" />

    </LinearLayout>
</FrameLayout>
